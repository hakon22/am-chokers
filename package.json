    "react-redux": "9.2.0",
    "react-toastify": "11.0.5",
    "react-verification-input": "4.2.2",
    "sharp": "0.34.5",
    "tsx": "4.21.0",
    "yup": "1.7.1"
  },
  "devDependencies": {
    "@a2seven/yoo-checkout": "1.1.4",
    "@eslint/eslintrc": "3.3.3",
