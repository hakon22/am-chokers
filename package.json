{
  "name": "am-chokers",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start:server:prod": "cross-env DB='HOST' NODE_ENV=production node server/server.js",
    "start:sender:prod": "cross-env DB='HOST' NODE_ENV=production node microservices/sender/src/main.js",
    "dev-prod": "cross-env DB='LOCAL' NODE_ENV=production tsx server/server.ts",
    "start:server:docker:dev": "cross-env DB='LOCAL' IS_DOCKER='TRUE' NODE_ENV=production node server/server.js",
    "start:sender:docker:dev": "cross-env DB='LOCAL' IS_DOCKER='TRUE' NODE_ENV=production node microservices/sender/src/main.js",
    "start:server:dev": "cross-env DB='LOCAL' NODE_ENV=development tsx server/server.ts",
    "start:sender:dev": "cross-env DB='LOCAL' NODE_ENV=development tsx microservices/sender/src/main.ts",
    "build": "next build",
    "lint": "eslint .",
    "migration:create": "typeorm migration:create",
    "migration:create:name": "tsx ./server/db/migrations/helpers/create.migration.helper.ts",
    "migration:run": "cross-env DB='LOCAL' CRON='TRUE' tsx ./node_modules/typeorm/cli.js migration:run --dataSource ./server/db/database.service.ts",
    "migration:run:prod": "cross-env DB='HOST' CRON='TRUE' NODE_ENV=production node ./node_modules/typeorm/cli.js migration:run --dataSource ./server/db/database.service.js",
    "migration:revert": "cross-env CRON='TRUE' tsx ./node_modules/typeorm/cli.js migration:revert --dataSource ./server/db/database.service.ts",
    "cron:promotional.status.control:run": "cross-env DB='HOST' CRON='TRUE' NODE_ENV=production node cron/promotional.status.control.js >> /srv/logs/promotional.status.control.log 2>&1",
    "cron:update-fids:run": "cross-env DB='HOST' CRON='TRUE' NODE_ENV=production node cron/update-fids.js >> /srv/logs/update-fids.log 2>&1",
    "cron:deferred-publication:run": "cross-env DB='HOST' CRON='TRUE' NODE_ENV=production node cron/deferred-publication.js >> /srv/logs/deferred-publication.log 2>&1",
    "cron:clear-user-refresh-tokens:run": "cross-env DB='HOST' CRON='TRUE' NODE_ENV=production node cron/clear-user-refresh-tokens.js >> /srv/logs/clear-user-refresh-tokens.log 2>&1"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "9.0.0",
    "@dnd-kit/sortable": "10.0.0",
    "@reduxjs/toolkit": "2.11.0",
    "animate.css": "4.1.1",
    "antd": "6.1.0",
    "antd-img-crop": "4.27.0",
    "antd-mask-input": "2.0.7",
    "aos": "2.3.4",
    "axios": "1.13.2",
    "bootstrap": "5.3.8",
    "classnames": "2.5.1",
    "i18next": "25.7.1",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "moment-timezone": "0.6.0",
    "next": "16.0.7",
    "rc-picker": "4.11.3",
    "react": "19.2.1",
    "react-bootstrap": "2.10.10",
    "react-bootstrap-icons": "1.11.6",
    "react-cookie-consent": "9.0.0",
