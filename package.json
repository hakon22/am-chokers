{
  "name": "am-chokers",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "backend",
    "microservices/sender"
  ],
  "scripts": {
    "start:server:prod": "cross-env DB='HOST' NODE_ENV=production tsx server/server.ts",
    "start:sender:prod": "cross-env DB='HOST' NODE_ENV=production tsx microservices/sender/src/main.ts",
    "dev-prod": "cross-env DB='LOCAL' NODE_ENV=production tsx server/server.ts",
    "start:server:dev": "cross-env DB='LOCAL' NODE_ENV=development tsx server/server.ts",
    "start:sender:dev": "cross-env DB='LOCAL' NODE_ENV=development tsx microservices/sender/src/main.ts",
    "build": "next build --no-lint",
    "lint": "next lint",
    "migration:create": "typeorm migration:create",
    "migration:create:name": "tsx ./server/db/migrations/helpers/create.migration.helper.ts",
    "migration:run": "cross-env DB='LOCAL' CRON='TRUE' tsx ./node_modules/typeorm/cli.js migration:run --dataSource ./server/db/database.service.ts",
    "migration:run:prod": "cross-env DB='HOST' CRON='TRUE' NODE_ENV=production tsx ./node_modules/typeorm/cli.js migration:run --dataSource ./server/db/database.service.ts",
    "migration:revert": "cross-env CRON='TRUE' tsx ./node_modules/typeorm/cli.js migration:revert --dataSource ./server/db/database.service.ts",
    "cron:promotional.status.control:run": "cross-env DB='HOST' CRON='TRUE' NODE_ENV=production tsx cron/promotional.status.control.ts >> /srv/logs/promotional.status.control.log 2>&1",
    "cron:update-fids:run": "cross-env DB='HOST' CRON='TRUE' NODE_ENV=production tsx cron/update-fids.ts >> /srv/logs/update-fids.log 2>&1",
    "cron:deferred-publication:run": "cross-env DB='HOST' CRON='TRUE' NODE_ENV=production tsx cron/deferred-publication.ts >> /srv/logs/deferred-publication.log 2>&1",
    "cron:clear-user-refresh-tokens:run": "cross-env DB='HOST' CRON='TRUE' NODE_ENV=production tsx cron/clear-user-refresh-tokens.ts >> /srv/logs/clear-user-refresh-tokens.log 2>&1"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@reduxjs/toolkit": "^2.3.0",
    "animate.css": "^4.1.1",
    "antd": "^5.21.6",
    "antd-img-crop": "^4.24.0",
    "antd-mask-input": "^2.0.7",
    "aos": "^2.3.4",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.3",
    "classnames": "^2.3.2",
    "i18next": "^23.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "next": "^15.1.0",
    "react": "^18",
    "react-bootstrap": "^2.9.1",
    "react-bootstrap-icons": "^1.10.3",
    "react-cookie-consent": "^9.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18",
    "react-i18next": "^14.0.5",
    "react-image-gallery": "^1.3.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-multi-carousel": "^2.8.6",
    "react-redux": "^9.0.4",
    "react-toastify": "^10.0.6",
    "react-verification-input": "^4.1.2",
    "sharp": "^0.33.0",
    "tsx": "^4.20.6",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@a2seven/yoo-checkout": "^1.1.4",
    "@stylistic/eslint-plugin-ts": "^2.10.1",
    "@types/aos": "^3.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/ffprobe-static": "^2.0.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.17.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.8.6",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18",
    "@types/react-bootstrap": "^0.32.35",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18",
    "@types/react-image-gallery": "^1.2.4",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.63.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.1",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "generate-password": "^1.7.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-verification-code": "^1.1.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "sass": "1.75",
    "telegraf": "^4.16.3",
    "typegram": "^5.2.0",
    "typeorm": "^0.3.27",
    "typescript": "^5.6.3",
    "typescript-ioc": "^3.2.2",
    "uuid": "^11.0.3",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5"
  }
}
